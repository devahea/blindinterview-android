<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ahea.blindinterview.android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".seek.main.SeekMainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".seek.detail.SeekDetailActivity" />
        <activity android:name=".seek.profile.ProfileActivity" />
        <activity android:name=".seek.resume.ResumeActivity" />
        <activity android:name=".seek.offer.OfferActivity" />
        <activity android:name=".seek.favorite.FavoriteActivity" />
        <activity
            android:name=".seek.setting.SettingsActivity"
            android:label="@string/title_activity_settings"></activity>
    </application>

</manifest>
